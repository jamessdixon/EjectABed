        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChickenSoftware.EjectABed.UserControl.WPF"
        mc:Ignorable="d"
        Title="Eject A Bed" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="targetTextBox" HorizontalAlignment="Left" Height="24" Margin="152,85,0,0" TextWrapping="Wrap" Text="Who do you want to eject?" VerticalAlignment="Top" Width="233"/>
